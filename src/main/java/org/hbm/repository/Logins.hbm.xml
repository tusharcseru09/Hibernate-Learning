<?xml version='1.0' encoding='utf-8'?>

<!--	***	Mapping Fiels	*** 
Hibernate mapping 
Hinernate mapping DTD 
-->

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


 
<hibernate-mapping>

	<class name="org.hbm.repository.Logins" table="LOGINS">
	
		<id name="LOGIN_ID" column="LOGIN_ID"> 
			<generator class="assigned" />
		</id>
		<property name="LOGIN_USERNAME" column="LOGIN_USERNAME"/>
		<property name="LOGIN_IP_ADDRESS" column="LOGIN_IP_ADDRESS"/>
	
	</class>

</hibernate-mapping>